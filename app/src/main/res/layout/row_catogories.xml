<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="catogoriesRowModel" type="com.buildcart.app.modules.catogories.data.model.CatogoriesRowModel"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumncategoryone"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylecornerRadius">
        <FrameLayout android:id="@+id/frameStack"
            android:layout_gravity="start"
            android:layout_height="@dimen/_82pxv"
            android:layout_width="@dimen/_100pxh" style="@style/groupStylewhite_A700cornerRadius">
            <ImageView android:id="@+id/imageImageFour"
                android:layout_gravity="center"
                android:layout_height="@dimen/_80pxh"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_11pxh"
                android:layout_marginTop="@dimen/_2pxv"
                android:layout_width="@dimen/_78pxh"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_image4}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_image4"/>
        </FrameLayout>
        <TextView android:ellipsize="end"
            android:id="@+id/txtCategoryOne"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_1pxv"
            android:layout_marginEnd="@dimen/_22pxh"
            android:layout_marginStart="@dimen/_22pxh"
            android:layout_marginTop="@dimen/_9pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{catogoriesRowModel.txtCategoryOne}"
            style="@style/txtNunitoregular10" tools:text="@string/lbl_category1"/>
    </LinearLayout>
</layout>
