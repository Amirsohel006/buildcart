<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="ordersFiveVM" type="com.buildcart.app.modules.ordersfive.data.viewmodel.OrdersFiveVM"/>
    </data>
    <LinearLayout android:id="@+id/linearOrdersFive"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar android:gravity="start"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowarrowleft"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylegray_800">
                <ImageView android:id="@+id/imageArrowleft"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_50pxv"
                    android:layout_width="@dimen/_14pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTrackOrder"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_8pxv"
                    android:layout_marginEnd="@dimen/_159pxh"
                    android:layout_marginStart="@dimen/_21pxh"
                    android:layout_marginTop="@dimen/_43pxv"
                    android:layout_weight="0.91"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{ordersFiveVM.ordersFiveModel.txtTrackOrder}"
                    style="@style/txtPoppinssemibold24" tools:text="@string/lbl_track_order"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>

        <FrameLayout android:id="@+id/frameStackorderconfirmed"
            android:layout_gravity="start"
            android:layout_height="@dimen/_264pxv"
            android:layout_marginEnd="@dimen/_28pxh"
            android:layout_marginStart="@dimen/_28pxh"
            android:layout_marginTop="@dimen/_38pxv" android:layout_width="@dimen/_147pxh">
            <RadioGroup android:id="@+id/rgRadiogrouporderconfirmed"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <RadioButton android:buttonTint="@color/gray_603"
                    android:ellipsize="end"
                    android:fontFamily="@font/poppins"
                    android:id="@+id/rbOrderConfirmed"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:paddingStart="@dimen/_12pxh"
                    android:singleLine="true"
                    android:text="@string/lbl_order_confirmed"
                    android:textColor="@color/gray_703"
                    android:textFontWeight="500"
                    android:textSize="@dimen/_14pxh"
                    android:textStyle="bold" tools:text="@string/lbl_order_confirmed"/>
                <RadioButton android:buttonTint="@color/gray_603"
                    android:ellipsize="end"
                    android:fontFamily="@font/poppins"
                    android:id="@+id/rbPacked"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_67pxh"
                    android:layout_marginTop="@dimen/_60pxv"
                    android:layout_width="wrap_content"
                    android:paddingStart="@dimen/_12pxh"
                    android:singleLine="true"
                    android:text="@string/lbl_packed"
                    android:textColor="@color/gray_703"
                    android:textFontWeight="500"
                    android:textSize="@dimen/_14pxh"
                    android:textStyle="bold" tools:text="@string/lbl_packed"/>
                <RadioButton android:buttonTint="@color/gray_603"
                    android:ellipsize="end"
                    android:fontFamily="@font/poppins"
                    android:id="@+id/rbDispatched"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_38pxh"
                    android:layout_marginTop="@dimen/_61pxv"
                    android:layout_width="wrap_content"
                    android:paddingStart="@dimen/_12pxh"
                    android:singleLine="true"
                    android:text="@string/lbl_dispatched"
                    android:textColor="@color/gray_703"
                    android:textFontWeight="500"
                    android:textSize="@dimen/_14pxh"
                    android:textStyle="bold" tools:text="@string/lbl_dispatched"/>
                <RadioButton android:buttonTint="@color/gray_603"
                    android:ellipsize="end"
                    android:fontFamily="@font/poppins"
                    android:id="@+id/rbOrderdelivered"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_9pxh"
                    android:layout_marginTop="@dimen/_58pxv"
                    android:layout_width="wrap_content"
                    android:paddingStart="@dimen/_12pxh"
                    android:singleLine="true"
                    android:text="@string/lbl_order_delivered"
                    android:textColor="@color/gray_703"
                    android:textFontWeight="500"
                    android:textSize="@dimen/_14pxh"
                    android:textStyle="bold" tools:text="@string/lbl_order_delivered"/>
            </RadioGroup>
            <View android:background="@color/gray_700"
                android:id="@+id/lineLineTwentyFour"
                android:layout_gravity="top|start"
                android:layout_height="@dimen/_66pxv"
                android:layout_marginBottom="@dimen/_18pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_8pxh"
                android:layout_marginTop="@dimen/_18pxv" android:layout_width="@dimen/_1pxh"/>
            <View android:background="@color/gray_700"
                android:id="@+id/lineLineTwentyFive"
                android:layout_gravity="start"
                android:layout_height="@dimen/_66pxv"
                android:layout_marginBottom="@dimen/_98pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_8pxh"
                android:layout_marginTop="@dimen/_98pxv" android:layout_width="@dimen/_1pxh"/>
            <View android:background="@color/gray_700"
                android:id="@+id/lineLineTwentySix"
                android:layout_gravity="bottom|start"
                android:layout_height="@dimen/_66pxv"
                android:layout_marginBottom="@dimen/_17pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_8pxh"
                android:layout_marginTop="@dimen/_17pxv" android:layout_width="@dimen/_1pxh"/>
            <LinearLayout android:id="@+id/linearRowlanguage"
                android:layout_gravity="top|end"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_22pxv"
                android:layout_marginEnd="@dimen/_3pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_22pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLanguage"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.59"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{ordersFiveVM.ordersFiveModel.txtLanguage}"
                    style="@style/txtPoppinsmedium10_2" tools:text="@string/lbl_26_may_tue"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTime"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_weight="0.41"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{ordersFiveVM.ordersFiveModel.txtTime}"
                    style="@style/txtPoppinsmedium10_2" tools:text="@string/lbl_04_35pm"/>
            </LinearLayout>
        </FrameLayout>
        <androidx.recyclerview.widget.RecyclerView
            android:gravity="start" android:id="@+id/recyclerOrdersFive"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_21pxh"
            android:layout_marginStart="@dimen/_21pxh"
            android:layout_marginTop="@dimen/_60pxv"
            android:layout_width="match_parent"
            android:orientation="vertical"
            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
            itemSpace="@{@dimen/_24pxh}" tools:itemCount="2" tools:listitem="@layout/row_orders_five"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowvectorTwo"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_21pxh"
            android:layout_marginStart="@dimen/_21pxh"
            android:layout_marginTop="@dimen/_26pxv"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <ImageView android:id="@+id/imageVectorTwo"
                android:layout_height="@dimen/_15pxh"
                android:layout_marginBottom="@dimen/_3pxv"
                android:layout_marginTop="@dimen/_2pxv"
                android:layout_width="@dimen/_12pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_vector_gray_705"
                tools:ignore="ContentDescription" tools:src="@drawable/img_vector_gray_705"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtDeliveryAddres"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_5pxh"
                android:layout_weight="0.91" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{ordersFiveVM.ordersFiveModel.txtDeliveryAddres}"
                style="@style/txtPoppinsmedium14_5" tools:text="@string/msg_delivery_addres"/>
        </LinearLayout>
        <TextView android:id="@+id/txtLanguageOne"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_21pxh"
            android:layout_marginStart="@dimen/_21pxh"
            android:layout_marginTop="@dimen/_11pxv"
            android:layout_width="wrap_content"
            android:text="@{ordersFiveVM.ordersFiveModel.txtLanguageOne}"
            style="@style/txtPoppinsregular14_6" tools:text="@string/msg_sgs_pg_2nd_st"/>
    </LinearLayout>
</layout>
