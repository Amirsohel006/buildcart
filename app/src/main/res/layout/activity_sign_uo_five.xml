<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="signUoFiveVM" type="com.buildcart.app.modules.signuofive.data.viewmodel.SignUoFiveVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearSignUoFive"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar android:gravity="start"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowarrowleft"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylegray_800">
                <ImageView android:id="@+id/imageArrowleft"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_30pxv"
                    android:layout_width="@dimen/_14pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtConfirmAddress"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_9pxv"
                    android:layout_marginEnd="@dimen/_98pxh"
                    android:layout_marginStart="@dimen/_21pxh"
                    android:layout_marginTop="@dimen/_28pxv"
                    android:layout_weight="0.94"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{signUoFiveVM.signUoFiveModel.txtConfirmAddress}"
                    style="@style/txtPoppinssemibold24" tools:text="@string/lbl_confirm_address"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>


        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnlineten"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_19pxh"
            android:layout_marginStart="@dimen/_19pxh"
            android:layout_marginTop="@dimen/_24pxv"
            android:layout_width="wrap_content" android:orientation="vertical">
            <LinearLayout android:id="@+id/linearRowone"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_3pxh"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearColumnone"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.14"
                    android:layout_width="0dp"
                    android:orientation="vertical" style="@style/groupStylegreen_800cornerRadius">
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtOne"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/_4pxv"
                        android:layout_marginEnd="@dimen/_15pxh"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{signUoFiveVM.signUoFiveModel.txtOne}"
                        style="@style/txtPoppinssemibold20_1" tools:text="@string/lbl_1"/>
                </LinearLayout>
                <View android:background="@color/amber_600"
                    android:id="@+id/lineLineTen"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginBottom="@dimen/_19pxv"
                    android:layout_marginTop="@dimen/_18pxv"
                    android:layout_weight="0.29" android:layout_width="0dp"/>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearColumntwo"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.14"
                    android:layout_width="0dp"
                    android:orientation="vertical" style="@style/groupStylecornerRadius_8">
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtTwo"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/_4pxv"
                        android:layout_marginEnd="@dimen/_13pxh"
                        android:layout_marginStart="@dimen/_13pxh"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{signUoFiveVM.signUoFiveModel.txtTwo}"
                        style="@style/txtPoppinssemibold20_3" tools:text="@string/lbl_2"/>
                </LinearLayout>
                <View android:background="@color/amber_600"
                    android:id="@+id/lineLineEleven"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginBottom="@dimen/_19pxv"
                    android:layout_marginTop="@dimen/_18pxv"
                    android:layout_weight="0.29" android:layout_width="0dp"/>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearColumnthree"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.14"
                    android:layout_width="0dp"
                    android:orientation="vertical" style="@style/groupStylecornerRadius_8">
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtThree"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/_4pxv"
                        android:layout_marginEnd="@dimen/_13pxh"
                        android:layout_marginStart="@dimen/_13pxh"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{signUoFiveVM.signUoFiveModel.txtThree}"
                        style="@style/txtPoppinssemibold20_3" tools:text="@string/lbl_3"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowaddress"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_4pxv"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtAddress"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_weight="0.25"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{signUoFiveVM.signUoFiveModel.txtAddress}"
                    style="@style/txtPoppinssemibold10" tools:text="@string/lbl_address"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtOrderSummary"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_78pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.48"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{signUoFiveVM.signUoFiveModel.txtOrderSummary}"
                    style="@style/txtPoppinsmedium10_1"
                    tools:text="@string/lbl_order_summary"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPayment"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_77pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.27"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{signUoFiveVM.signUoFiveModel.txtPayment}"
                    style="@style/txtPoppinsmedium10_1" tools:text="@string/lbl_payment"/>
            </LinearLayout>
        </LinearLayout>


        <ScrollView
            android:id="@+id/scrollview"
            android:fillViewport="true"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="0dp">

        <LinearLayout
            android:id="@+id/forscrollview"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        <TextView android:ellipsize="end"
            android:id="@+id/txtSavedAddress"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_23pxh"
            android:layout_marginStart="@dimen/_23pxh"
            android:layout_marginTop="@dimen/_34pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{signUoFiveVM.signUoFiveModel.txtSavedAddress}"
            style="@style/txtPoppinsmedium20_1" tools:text="@string/lbl_saved_address"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowhome"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_19pxh"
            android:layout_marginStart="@dimen/_19pxh"
            android:layout_marginTop="@dimen/_17pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylewhite_9">
            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recyclerview"
                android:layout_width="match_parent"
                android:layout_height="match_parent"/>
        </LinearLayout>
        <LinearLayout android:gravity="center_vertical"
            android:id="@+id/linearRowfloatingicon"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_19pxh"
            android:layout_marginStart="@dimen/_19pxh"
            android:layout_marginTop="@dimen/_57pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylecornerRadius_9">
            <ImageView android:id="@+id/imageFloatingicon"
                android:layout_height="@dimen/_20pxh"
                android:layout_marginBottom="@dimen/_15pxv"
                android:layout_marginStart="@dimen/_62pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_width="@dimen/_20pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_floatingicon"
                tools:ignore="ContentDescription" tools:src="@drawable/img_floatingicon"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtAddNewAddress"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_13pxv"
                android:layout_marginEnd="@dimen/_62pxh"
                android:layout_marginStart="@dimen/_6pxh"
                android:layout_marginTop="@dimen/_8pxv"
                android:layout_weight="0.89" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{signUoFiveVM.signUoFiveModel.txtAddNewAddress}"
                style="@style/txtNunitobold20_2" tools:text="@string/lbl_add_new_address"/>
        </LinearLayout>
        <androidx.appcompat.widget.AppCompatButton
            android:ellipsize="end"
            android:gravity="center"
            android:layout_gravity="bottom"
            android:id="@+id/btnNext"
            android:layout_marginTop="30dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_17pxh"
            android:layout_marginStart="@dimen/_19pxh"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_16pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_16pxv" android:singleLine="true"
            android:text="@string/lbl_next" android:textAllCaps="false"
            style="@style/btnSolidRounded_6" tools:text="@string/lbl_next"/>

        </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>
