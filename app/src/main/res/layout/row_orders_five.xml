<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="ordersFiveRowModel" type="com.buildcart.app.modules.ordersfive.data.model.OrdersFiveRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRowvector"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="horizontal">
        <ImageView android:id="@+id/imageVector"
            android:layout_height="@dimen/_14pxh"
            android:layout_marginBottom="@dimen/_4pxv"
            android:layout_marginTop="@dimen/_4pxv"
            android:layout_width="@dimen/_14pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_vector_gray_704"
            tools:ignore="ContentDescription" tools:src="@drawable/img_vector_gray_704"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtExpectedDelive"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_1pxv"
            android:layout_marginStart="@dimen/_5pxh"
            android:layout_marginTop="@dimen/_1pxv"
            android:layout_weight="0.63" android:layout_width="0dp"
            android:singleLine="true"
            android:text="@{ordersFiveRowModel.txtExpectedDelive}"
            style="@style/txtPoppinsmedium14_5" tools:text="@string/msg_expected_delive"/>
        <TextView android:ellipsize="end" android:id="@+id/txt27MayWed"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_11pxh"
            android:layout_marginTop="@dimen/_1pxv"
            android:layout_weight="0.32" android:layout_width="0dp"
            android:singleLine="true"
            android:text="@{ordersFiveRowModel.txt27MayWed}"
            style="@style/txtPoppinsregular14_5" tools:text="@string/lbl_27_may_wed"/>
    </LinearLayout>
</layout>
