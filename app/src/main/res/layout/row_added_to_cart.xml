<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools">


    <LinearLayout android:gravity="start"
        android:id="@+id/linearRowuntitled1080"
        android:layout_height="@dimen/_150pxh"
        android:layout_marginEnd="@dimen/_19pxh"
        android:layout_marginStart="@dimen/_19pxh"
        android:layout_marginTop="@dimen/_13pxv"
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:background="@drawable/background_for_cart">
        <FrameLayout android:id="@+id/frameStackuntitled1080"
            android:layout_height="@dimen/_111pxv"
            android:layout_marginBottom="@dimen/_13pxv"
            android:layout_marginStart="@dimen/_3pxh"
            android:layout_marginTop="@dimen/_8pxv"
            android:layout_width="@dimen/_90pxh" style="@style/groupStylewhite_4">
            <ImageView android:id="@+id/imageUntitled1080"
                android:layout_gravity="center"
                android:layout_height="@dimen/_106pxh"
                android:layout_marginEnd="@dimen/_5pxh"
                android:layout_marginStart="@dimen/_5pxh"
                android:layout_marginTop="@dimen/_4pxv"
                android:layout_width="@dimen/_80pxh"
                android:scaleType="fitXY"
                tools:ignore="ContentDescription"
                tools:src="@drawable/img_untitled1080"/>
        </FrameLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnorderid23475"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_10pxv"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_4pxv"
            android:layout_weight="0.58"
            android:layout_width="0dp" android:orientation="vertical">
            <TextView android:ellipsize="end"
                android:id="@+id/txtOrderID23475"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@string/msg_order_id_23475"
                style="@style/txtPoppinsmedium14"
                tools:text="@string/msg_order_id_23475"/>
            <FrameLayout android:id="@+id/frameStackdesigntiles"
                android:layout_height="@dimen/_46pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="@dimen/_85pxh">
                <TextView android:ellipsize="end"
                    android:gravity="top|start"
                    android:id="@+id/txtDesignTiles"
                    android:layout_gravity="top|center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_1pxh"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@string/lbl_design_tiles"
                    style="@style/txtPoppinsmedium14_1"
                    tools:text="@string/lbl_design_tiles"/>
                <TextView android:ellipsize="end"
                    android:gravity="top|start"
                    android:id="@+id/txtCreamColour"
                    android:layout_gravity="bottom|center"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{myCartVM.myCartModel.txtCreamColour}"
                    style="@style/txtPoppinsregular12_1" tools:text="@string/lbl_cream_colour"/>
            </FrameLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowlanguage"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLanguage"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_qty2"
                    style="@style/txtPoppinsregular14_1"
                    tools:text="@string/lbl_qty2"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtGroup434"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_3pxh"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_1pxv"
                    android:paddingStart="@dimen/_8pxh"
                    android:paddingTop="@dimen/_1pxv"
                    android:singleLine="true"
                    android:text="@{myCartVM.myCartModel.txtGroup434}"
                    style="@style/txtRoundedOutline_1" tools:text="@string/lbl_100"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtPrice"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{myCartVM.myCartModel.txtPrice}"
                style="@style/txtPoppinssemibold14_2"
                tools:text="@string/lbl_17000"/>
        </LinearLayout>

        <ImageView
            android:layout_marginTop="10dp"
            android:layout_marginEnd="15dp"
            android:id="@+id/image"
            android:src="@drawable/img_close"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
    </LinearLayout>


</LinearLayout>