<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="signUoElevenVM" type="com.buildcart.app.modules.signuoeleven.data.viewmodel.SignUoElevenVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearSignUoEleven"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical"
        style="@style/groupStylewhite_A700">


        <ScrollView
            android:id="@+id/scrollview"
            android:fillViewport="true"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        <FrameLayout android:id="@+id/frameStackellipseone"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_161pxv"
            android:layout_marginEnd="@dimen/_19pxh"
            android:layout_marginStart="@dimen/_19pxh"
            android:layout_marginTop="@dimen/_56pxv" android:layout_width="@dimen/_131pxh">
            <ImageView
                android:src="@drawable/rounded_profile_image"
                android:id="@+id/imageViewEllipseOne"
                android:layout_gravity="start"
                android:scaleType="centerCrop"
                android:layout_height="@dimen/_159pxv"
                android:layout_width="@dimen/_131pxh"/>
            <ImageView android:id="@+id/imageVector"
                android:layout_gravity="bottom|center"
                android:layout_height="@dimen/_32pxh"
                android:layout_width="@dimen/_32pxh"
                android:scaleType="fitXY"
                android:background="@drawable/img_vector_black_900"
                tools:ignore="ContentDescription"
                tools:src="@drawable/img_vector_black_900"/>
        </FrameLayout>
        <TextView android:ellipsize="end"
            android:id="@+id/txtAddProfile"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_19pxh"
            android:layout_marginStart="@dimen/_19pxh"
            android:layout_marginTop="@dimen/_6pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{signUoElevenVM.signUoElevenModel.txtAddProfile}"
            style="@style/txtNunitoregular16_1" tools:text="@string/lbl_add_profile"/>
        <EditText
            android:background="@drawable/rectangle_border_gray_601_radius_5"
            android:ellipsize="end" android:hint="@string/lbl_full_name"
            android:id="@+id/etFullName" android:inputType="text"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_19pxh"
            android:layout_marginStart="@dimen/_19pxh"
            android:layout_marginTop="@dimen/_27pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_14pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_17pxh"
            android:paddingTop="@dimen/_13pxv" android:singleLine="true"
            android:text="@={signUoElevenVM.signUoElevenModel.etGroup421Value}"
            android:textColorHint="@color/gray_800"
            style="@style/etRoundedOutline_2" tools:ignore="Autofill"/>
        <TextView
            android:background="@drawable/rectangle_border_gray_601_radius_5"
            android:ellipsize="end"
            android:text="@string/lbl_mobile_number"
            android:id="@+id/etMobileNumber"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_19pxh"
            android:layout_marginStart="@dimen/_19pxh"
            android:layout_marginTop="@dimen/_14pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_14pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_17pxh"
            android:paddingTop="@dimen/_13pxv" android:singleLine="true"
            android:textColorHint="@color/gray_800"
            style="@style/etRoundedOutline_2" tools:ignore="Autofill"/>
        <EditText
            android:background="@drawable/rectangle_border_gray_601_radius_5"
            android:ellipsize="end" android:hint="@string/lbl_email_id"
            android:id="@+id/etEmailId"
            android:inputType="textEmailAddress"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_19pxh"
            android:layout_marginStart="@dimen/_19pxh"
            android:layout_marginTop="@dimen/_14pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_14pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_17pxh"
            android:paddingTop="@dimen/_13pxv" android:singleLine="true"
            android:text="@={signUoElevenVM.signUoElevenModel.etGroup423Value}"
            android:textColorHint="@color/gray_800"
            style="@style/etRoundedOutline_2" tools:ignore="Autofill"/>
        <EditText
            android:background="@drawable/rectangle_border_gray_601_radius_5"
            android:ellipsize="end" android:hint="@string/lbl_city"
            android:id="@+id/etCity" android:inputType="text"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_19pxh"
            android:layout_marginStart="@dimen/_19pxh"
            android:layout_marginTop="@dimen/_14pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_13pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_17pxh"
            android:paddingTop="@dimen/_14pxv" android:singleLine="true"
            android:text="@={signUoElevenVM.signUoElevenModel.etGroup424Value}"
            android:textColorHint="@color/gray_800"
            style="@style/etRoundedOutline_2" tools:ignore="Autofill"/>
        <EditText
            android:background="@drawable/rectangle_border_gray_601_radius_5"
            android:ellipsize="end" android:hint="@string/lbl_pincode"
            android:id="@+id/etPincode" android:inputType="number"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_19pxh"
            android:layout_marginStart="@dimen/_19pxh"
            android:layout_marginTop="@dimen/_14pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_14pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_17pxh"
            android:paddingTop="@dimen/_13pxv" android:singleLine="true"
            android:text="@={signUoElevenVM.signUoElevenModel.etGroup425Value}"
            android:textColorHint="@color/gray_800"
            style="@style/etRoundedOutline_2" tools:ignore="Autofill"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowgroupfortyfive"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_33pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <CheckBox
                android:background="@drawable/rectangle_border_gray_800_radius_5"
                android:id="@+id/checkboxTermsAndConditions"
                android:layout_height="@dimen/_25pxh"
                android:layout_width="@dimen/_25pxh"
                android:paddingHorizontal="@dimen/_5pxh"
                android:paddingVertical="@dimen/_5pxv"
                android:scaleType="fitCenter"
                android:src="@drawable/img_group45" tools:ignore="ContentDescription"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtTermAndCondition"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginStart="@dimen/_11pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_weight="0.9" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{signUoElevenVM.signUoElevenModel.txtTermAndCondition}"
                style="@style/txtPoppinsregular16_2" tools:text="@string/msg_accept_all_term"/>
        </LinearLayout>
        <ProgressBar
            android:visibility="gone"
            android:id="@+id/progresBar"
            android:layout_gravity="center_horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <androidx.appcompat.widget.AppCompatButton
            android:ellipsize="end" android:gravity="center"
            android:id="@+id/btnComplete"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_19pxh"
            android:layout_marginStart="@dimen/_19pxh"
            android:layout_marginTop="@dimen/_26pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_16pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_16pxv" android:singleLine="true"
            android:text="@string/lbl_complete"
            android:textAllCaps="false" style="@style/btnSolidRounded_3" tools:text="@string/lbl_complete"/>

        </LinearLayout>

        </ScrollView>
    </LinearLayout>
</layout>
